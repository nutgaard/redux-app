{
  "name": "redux-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-intl": "^2.5.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^2.0.4"
  },
  "scripts": {
    "start": "npm-run-all build:text -s start:*",
    "start:script": "react-scripts-ts start",
    "start:text": "chokidar \"i18n/**/*\" -c \"yarn text:build\"",
    "build": "run-s build:text build:script",
    "build:script": "react-scripts-ts build",
    "build:text": "react-intl-bundler i18n src --typescript",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@navikt/react-intl-bundler": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.11",
    "@types/react-redux": "^6.0.9",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "chokidar-cli": "^1.2.1",
    "npm-run-all": "^4.1.3",
    "typescript": "^3.0.3"
  }
}
