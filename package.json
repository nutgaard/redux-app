{
  "name": "redux-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-intl": "^2.5.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "cross-env REACT_APP_MOCK=false npm-run-all builder:text -p watch:*",
    "start:mock": "cross-env REACT_APP_MOCK=true npm-run-all builder:text -p watch:*",
    "watch:script": "react-scripts-ts start",
    "watch:text": "chokidar \"i18n/**/*\" -c \"yarn builder:text\"",
    "build": "cross-env REACT_APP_MOCK=false run-s builder:text builder:script",
    "build:mock": "cross-env REACT_APP_MOCK=true run-s builder:text builder:script",
    "builder:script": "react-scripts-ts build",
    "builder:text": "react-intl-bundler i18n src --typescript",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@navikt/react-intl-bundler": "^1.0.3",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.11",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.31",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/reselect": "^2.2.0",
    "chokidar-cli": "^1.2.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest-enzyme": "^6.0.4",
    "npm-run-all": "^4.1.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.1",
    "typescript": "^3.0.3",
    "yet-another-fetch-mock": "^2.3.3"
  }
}
